{
    "metrics": {
        "line_code_coverage": {
            "formula": "(covered_lines / total_lines) * 100",
            "unit": "percentage",
            "description": "Percentage of total executable lines covered by tests."
        },
        "pipeline_duration_s": {
            "formula": "pipelineDurationMs / 1000",
            "unit": "seconds"
        }
    }
}